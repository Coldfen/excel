{
  "parser": "@babel/eslint-parser",
  "env" : {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "linebreak-style": "off",
    "eol-last": "off",
    "indent": "off",
    "padded-blocks": "off",
    "operator-linebreak": "off",
    "no-debugger": "off"
  },
  "extends": ["eslint:recommended", "google"],
  "plugins": [
    "babel"
    ]
}
